    <script>
      {`
        (function() {
          const heroCta = document.querySelector('a[href="#cta-bottom"]');
          if (!heroCta) return;

          const getCookie = (name) => {
            const match = document.cookie.match(new RegExp('(^| )' + name + '=([^;]+)'));
            return match ? match[2] : undefined;
          };

          heroCta.addEventListener('click', async () => {
            const event_id = crypto.randomUUID ? crypto.randomUUID() : Math.random().toString(36).slice(2);
            const fbp = getCookie('_fbp');
            const fbc = getCookie('_fbc');

            // Browser pixel (gtag) with dedup event_id if available
            if (window.gtag) {
              window.gtag('event', 'add_to_cart', { event_id });
            }

            // Send server-side CAPI
            try {
              await fetch('/api/meta-capi', {
                method: 'POST',
                headers: { 'Content-Type': 'application/json' },
                body: JSON.stringify({
                  event_name: 'AddToCart',
                  event_id,
                  event_source_url: window.location.href,
                  fbp,
                  fbc,
                  custom_data: { value: 0, currency: 'USD' }
                }),
              });
            } catch (e) {
              console.error('CAPI send failed', e);
            }
          });
        })();
      `}
